                "initialConfigurations": [
                    {
                        "type": "probe-rs-debug",
                        "request": "launch",
                        "name": "probe-rs Test",
                        "cwd": "${workspaceFolder}",
                        "connectUnderReset": true,
                        "chip": "STM32H745ZITx",
                        "flashingConfig": {
                            "flashingEnabled": true,
                            "haltAfterReset": true
                        },
                        "coreConfigs": [
