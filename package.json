                        "required": [
                            "chip",
                            "coreConfigs"
                        ],
                        "properties": {
                            "server": {
                                "type": "string",
                                "description": "Optionally connect to an existing `probe-rs dap-server` session on IP and Port, e.g. '127.0.0.1:50000'",
